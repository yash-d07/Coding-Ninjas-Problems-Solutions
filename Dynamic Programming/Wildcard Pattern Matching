#include<bits/stdc++.h>
bool wildcardMatching(string pattern, string text)
{
   int n = pattern.size();
   int m = text.size();
   vector<vector<bool>> dp(n+1,vector<bool> (m+1,false));
    dp[0][0] = true;
   for(int i=1;i<=n;i++) {
              dp[i][0] = true;
      for(int a=1;a<=i;a++)
        {
                    if(pattern[a-1] != '*') dp[i][0] = false;
        }
   }
   for(int j=1;j<=m;j++) dp[0][j] = false;
    
    for(int i=1;i<=n;i++)
    {
        for(int j=1; j<=m; j++)
        {
             if(pattern[i-1] == text[j-1] || pattern[i-1] == '?'){
             dp[i][j] = dp[i-1][j-1];
             }
            else if(pattern[i-1] == '*')
                dp[i][j] = dp[i-1][j] | dp[i][j-1];     
            else
                dp[i][j] = false;
        }
    }
    return dp[n][m];
}
